{
  "name": "Remove StartPage Ads",
  "description": "Google extension that removes ads from all webpages matching startpage.com.",
  "manifest_version": 2,
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "homepage_url": "https://github.com/paulshryock/remove-startpage-ads-google#readme",
  "default_locale": "en",
  "permissions": [
    "*://*.startpage.com/*"
  ]
}